{
  "name": "wakatime-save-history",
  "version": "0.1.0",
  "description": "Engine for save the Wakatime status from API",
  "main": "src/server.ts",
  "repository": "https://github.com/ricovilela/wakatime-save-history.git",
  "author": "Rico Vilela",
  "private": false,
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "build": "tsc && copyfiles -u 1 src/views/* dist && copyfiles -u 1 src/views/partials/* dist && copyfiles -u 1 src/database/config/* dist",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "bower": "^1.8.12",
    "child_process": "^1.0.2",
    "consolidate": "^0.16.0",
    "dateformat": "^4.5.1",
    "dotenv": "^9.0.0",
    "dust": "^0.3.0",
    "dustjs-helpers": "^1.7.4",
    "dustjs-linkedin": "^2.7.5",
    "express": "^4.17.1",
    "mariadb": "^2.5.3",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.3",
    "@commitlint/config-conventional": "^12.1.3",
    "@types/dustjs-linkedin": "^1.2.31",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/md5": "^2.3.0",
    "@types/passport": "^1.0.6",
    "@types/sequelize": "^4.28.9",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.24.0",
    "commitizen": "^4.2.4",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix --quiet 'src/**/*.ts'",
      "prettier --write --ignore-unknown './**/*.ts' "
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
